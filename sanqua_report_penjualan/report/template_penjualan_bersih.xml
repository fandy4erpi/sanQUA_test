<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data>

		<template id="penjualan_bersih_template_report_view">
			<t t-call="web.external_layout">
				<t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})"/>
				<div class="page">
					<br/>
					<div class="oe_structure"/>
						<div>
							<h2 class="text-center mb32">
								<strong>Penjualan Bersih</strong>
							</h2>
						</div>
					<br/>

				<br/>

				<table style="width:100%;margin:0px auto;background:white;border:1px solid #e1e1e1;" class="table table-condensed">
					<thead>
						<tr>
							<th>DO Number</th>
							<th>SO Number</th>
							<th>Customer Name</th>
							<th>Customer Address</th>
							<th>Delivery Date</th>
							<th>Company</th>
							<th>Product</th>
							<th>Demand</th>
							<th>Done</th>
							<th>Return</th>
							<th>Net (Done â€“ Return)</th>
							<th>No. Invoice</th>
							<th>Total Invoice</th>
						</tr>
					</thead>
					<tbody class="consumed_tbody">
						<t t-foreach="o.print_xxx()" t-as="line">
						<tr>
							<td>
								<span t-esc="line[0]" />
							</td>
							<td>
								<span t-esc="line[1]" />
							</td>
							<td>
								<span t-esc="line[2]" />
							</td>
							<td>
								<span t-esc="line[3]" />
							</td>
							<td>
								<span t-esc="line[4]" />
							</td>
							<td>
								<span t-esc="line[5]" />
							</td>
							<td>
								<span t-esc="line[6]" />
							</td>
							<td>
								<span t-esc="line[7]" />
							</td>
							<td>
								<span t-esc="line[8]" />
							</td>
							<td>
								<span t-esc="line[9]" />
							</td>
							<td>
								<span t-esc="line[10]" />
							</td>
							<td>
								<span t-esc="line[11]" />
							</td>
							<td>
								<span t-esc="line[12]" />
							</td>

						</tr>
						</t>
					</tbody>
				</table>
				</div>
			</t>
		</template>

		<template id="penjualan_bersih_report_template_id">
			<t t-call="web.html_container">
				<t t-foreach="docs" t-as="o">
					<t t-call="sanqua_report_penjualan.penjualan_bersih_template_report_view"  t-lang="o.partner_id.lang"/>
				</t>
			</t>
		</template>

	</data>
</odoo>